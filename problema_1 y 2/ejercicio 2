 var x= Math.floor(Math.random() * 6 + 1);
 var y= Math.floor(Math.random() * 6 + 1);
  r=(x*1)+(y*1);
    console.log("La suma es: ", r);
      r=(x*1)-(y*1);
    console.log("La resta es: ", r);
      r=(x*1)*(y*1);
    console.log("La multiplicacion es: ", r);
      r=(x*1)/(y*1);
    console.log("La divicion es: ", r);
